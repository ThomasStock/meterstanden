{
  "name": "meterstanden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:live": "dotenv -e .env.live -- npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run db:generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:live": "dotenv -e .env.live -- npx prisma db push",
    "db:start": "colima start && docker start meters",
    "db:stop": "colima stop",
    "docker:run": "docker run --name meters -p 5432:5432 -e POSTGRES_PASSWORD=meterstanden -d postgres"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
