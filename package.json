    "@mui/x-date-pickers": "^5.0.4",
    "@prisma/client": "^4.4.0",
    "@tanstack/query-sync-storage-persister": "^4.12.0",
    "@tanstack/react-query": "^4.12.0",
    "@tanstack/react-query-persist-client": "^4.12.0",
    "@trpc/client": "^10.0.0-proxy-beta.19",
    "@trpc/next": "^10.0.0-proxy-beta.19",
    "@trpc/react": "^10.0.0-proxy-beta.19",
    "@trpc/server": "^10.0.0-proxy-beta.19",
    "@types/luxon": "^3.0.1",
    "babel-plugin-superjson-next": "^0.4.4",
